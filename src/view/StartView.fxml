<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Controller">
   <children>
      <VBox layoutX="3.0" layoutY="4.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0">
         <children>
            <BorderPane fx:id="settingsPane">
               <top>
                  <Label text="Simulation Controls" BorderPane.alignment="BOTTOM_LEFT" />
               </top>
               <right>
                  <HBox spacing="3.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button fx:id="startButton" disable="true" mnemonicParsing="false" onAction="#startButtonClicked" text="Start" />
                        <Button fx:id="pauseButton" disable="true" mnemonicParsing="false" text="Pause" />
                     </children>
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </HBox>
               </right>
               <center>
                  <HBox spacing="2.0">
                     <children>
                        <Slider fx:id="simulationStepSlider" blockIncrement="1.0" cache="true" maxWidth="1.7976931348623157E308" minorTickCount="5" onMouseClicked="#simulationStepSliderChanged" showTickLabels="true" showTickMarks="true" HBox.hgrow="ALWAYS">
                           <padding>
                              <Insets right="5.0" />
                           </padding>
                        </Slider>
                        <Button fx:id="previousStepButton" mnemonicParsing="false" onAction="#onStepButtonPressed" text="&lt;" />
                        <Button fx:id="nextStepButton" mnemonicParsing="false" onAction="#onStepButtonPressed" text="&gt;" />
                     </children>
                  </HBox>
               </center>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </BorderPane>
            <ScrollPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefViewportWidth="644.0">
               <content>
                  <Pane fx:id="simulationVisualizationPane" />
               </content>
            </ScrollPane>
            <BorderPane fx:id="resultView">
               <bottom>
                  <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="3.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button mnemonicParsing="false" text="Choose Path" />
                        <Label maxWidth="1.7976931348623157E308" text="Path:" textAlignment="CENTER" />
                        <TextField disable="true" HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" text="Save" BorderPane.alignment="CENTER" />
                     </children>
                     <padding>
                        <Insets top="3.0" />
                     </padding>
                  </HBox>
               </bottom></BorderPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
